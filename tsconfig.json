{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "src/",
    "outDir": "out/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "include": ["./src/"]
}
